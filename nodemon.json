{
  "restartable": "rs",
  "ignore": [
    ".*",
    "node_modules/**/node_modules",
    "views/**",
    "public/stylesheets/**",
    "public/javascripts/**"
  ],
  "verbose": true,
  "execMap": {

  },
  "events": {
    "restart": "osascript -e 'display notification \"OneToMany restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}